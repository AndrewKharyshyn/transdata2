            axios.post(`/api/agency`, params)\r\n                    .then(response => {\r\n                        this.response = response.data;\r\n                        this.agency.id = response.data;\r\n                        console.log(response.data);\r\n                        this.showResponse = true\r\n                    })\r\n                    .catch(e => {\r\n                        this.errors.push(e)\r\n                    })\r\n            },\r\n            clear(field) {\r\n                this[field] = ''\r\n            },\r\n        },\r\n    }\r\n</script>\r\n"]}]}