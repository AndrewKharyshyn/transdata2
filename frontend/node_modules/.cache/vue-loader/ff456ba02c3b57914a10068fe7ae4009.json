   if (!this.term || this.term.length < 1) {\r\n                    return this.files\r\n                }\r\n\r\n                return this.files.filter(item => {\r\n                    return item.fileName.toLowerCase().includes(this.term.toLowerCase())\r\n                })\r\n            },\r\n        },\r\n        methods: {\r\n            showOrder(order) {\r\n                alert(JSON.stringify(order))\r\n            },\r\n            search: debounce(function (term) {\r\n                this.term = term\r\n            }, 400),\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}