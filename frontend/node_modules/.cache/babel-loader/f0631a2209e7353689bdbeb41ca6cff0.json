1BA;AA1EA,CAAA","sourcesContent":["<template>\r\n  <va-card :title=\"$t('tables.latestOrders')\">\r\n    <div class=\"row align--center\">\r\n      <div class=\"flex xs12 md6\">\r\n        <va-input\r\n          :value=\"term\"\r\n          :placeholder=\"$t('tables.search')\"\r\n          @input=\"search\"\r\n        >\r\n          <va-icon name=\"fa fa-search\" slot=\"prepend\"/>\r\n        </va-input>\r\n      </div>\r\n\r\n      <div class=\"flex xs12 md3 offset--md3\">\r\n        <va-select\r\n          v-model=\"perPage\"\r\n          :label=\"$t('tables.perPage')\"\r\n          :options=\"perPageOptions\"\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n    <va-data-table\r\n      :fields=\"fields\"\r\n      :data=\"filteredData\"\r\n      :per-page=\"parseInt(perPage)\"\r\n      @row-clicked=\"showOrder\"\r\n      clickable\r\n      striped\r\n      hoverable\r\n    >\r\n      <template slot=\"marker\" slot-scope=\"props\">\r\n        <va-icon name=\"fa fa-circle\" :color=\"props.rowData.color\" size=\"8px\"/>\r\n      </template>\r\n\r\n      <template slot=\"trend\" slot-scope=\"props\">\r\n        <va-icon :name=\"getTrendIcon(props.rowData)\" :color=\"getTrendColor(props.rowData)\"/>\r\n      </template>\r\n\r\n      <template slot=\"status\" slot-scope=\"props\">\r\n        <va-badge :color=\"getStatusColor(props.rowData)\">\r\n          {{ props.rowData.status }}\r\n        </va-badge>\r\n      </template>\r\n\r\n      <template slot=\"actions\" slot-scope=\"props\">\r\n        <va-popover :message=\"`${$t('tables.copy')} ${props.rowData.code}`\" placement=\"left\">\r\n          <va-button flat small color=\"gray\" icon=\"fa fa-files-o\"/>\r\n        </va-popover>\r\n\r\n        <va-popover :message=\"`${$t('tables.files')} ${props.rowData.code}`\" placement=\"left\">\r\n          <va-button flat small color=\"gray\" icon=\"fa fa-file-text-o\"/>\r\n        </va-popover>\r\n      </template>\r\n    </va-data-table>\r\n  </va-card>\r\n</template>\r\n\r\n<script>\r\n  import {debounce} from 'lodash'\r\n  import orders from '../../../data/allOrders.json'\r\n\r\n  export default {\r\n    data() {\r\n      return {\r\n        term: null,\r\n        perPage: '3',\r\n        perPageOptions: ['2', '3', '4', '5'],\r\n        orders: orders,\r\n      }\r\n    },\r\n    computed: {\r\n      fields() {\r\n        return [{\r\n          name: '__slot:marker',\r\n          width: '3%',\r\n        }, {\r\n          name: 'code',\r\n          title: this.$t('tables.headings.code'),\r\n          width: '10%',\r\n          sortField: 'code',\r\n        }, {\r\n          name: 'description',\r\n          title: this.$t('tables.headings.description'),\r\n          width: '20%',\r\n          sortField: 'description',\r\n        }, {\r\n          name: 'agency',\r\n          title: this.$t('tables.headings.agency'),\r\n          width: '15%',\r\n          sortField: 'agency',\r\n        }, {\r\n          name: 'orderSum',\r\n          title: this.$t('tables.headings.orderSum'),\r\n          width: '10%',\r\n          sortField: 'orderSum',\r\n        }, {\r\n          name: 'orderDate',\r\n          title: this.$t('tables.headings.orderDate'),\r\n          width: '15%',\r\n          sortField: 'orderDate',\r\n        }, {\r\n          name: '__slot:status',\r\n          title: this.$t('tables.headings.status'),\r\n          width: '15%',\r\n          sortField: 'status',\r\n        }, {\r\n          name: 'symbols',\r\n          title: this.$t('tables.headings.symbols'),\r\n          width: '5%',\r\n          sortField: 'symbols',\r\n        }, {\r\n          name: 'pages',\r\n          title: this.$t('tables.headings.pages'),\r\n          width: '5%',\r\n          sortField: 'pages',\r\n        }, {\r\n          name: 'language',\r\n          title: this.$t('tables.headings.language'),\r\n          width: '5%',\r\n          sortField: 'language',\r\n        }, {\r\n          name: '__slot:actions',\r\n          dataClass: 'text-right',\r\n        }]\r\n      },\r\n      filteredData() {\r\n        if (!this.term || this.term.length < 1) {\r\n          return this.orders\r\n        }\r\n\r\n        return this.orders.filter(item => {\r\n          return item.description.toLowerCase().includes(this.term.toLowerCase())\r\n        })\r\n      },\r\n    },\r\n    methods: {\r\n      showOrder(order) {\r\n        alert(JSON.stringify(order))\r\n      },\r\n      getStatusColor(order) {\r\n        if (order.status === 'Pending') {\r\n\r\n          return 'info'\r\n        }\r\n        if (order.status === 'In progress') {\r\n          return 'danger'\r\n        }\r\n        if (order.status === 'Editing') {\r\n          return 'warning'\r\n        }\r\n        if (order.status === 'Proofreading') {\r\n          return 'secondary'\r\n        }\r\n        if (order.status === 'Finished') {\r\n          return 'primary'\r\n        }\r\n        if (order.status === 'Paid') {\r\n          return 'grey'\r\n        }\r\n        return 'grey'\r\n      },\r\n      search: debounce(function (term) {\r\n        this.term = term\r\n      }, 400),\r\n    },\r\n  }\r\n</script>\r\n"],"sourceRoot":"src/components/order-lists/order-tables"}]}