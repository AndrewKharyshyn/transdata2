urcesContent":["<template>\r\n  <div class=\"app-page-layout\">\r\n    <slot></slot>\r\n    <slot name=\"content\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: 'app-page-layout',\r\n  props: {\r\n    mobileWidth: {\r\n      type: Number,\r\n      default: 767,\r\n    },\r\n  },\r\n  mounted () {\r\n    window.addEventListener('resize', () => this.updateSidebarState())\r\n\r\n    this.updateSidebarState()\r\n  },\r\n  beforeDestroy () {\r\n    window.removeEventListener('resize', () => this.updateSidebarState())\r\n  },\r\n  methods: {\r\n    checkIsDesktop () {\r\n      return window.matchMedia(`(min-width: ${this.mobileWidth}px)`).matches\r\n    },\r\n    updateSidebarState () {\r\n      if (this.checkIsDesktop()) {\r\n        this.$emit('update:minimized', false)\r\n      } else {\r\n        this.$emit('update:minimized', true)\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n.app-page-layout {\r\n  position: fixed;\r\n  top: 0;\r\n  right: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  height: 100vh;\r\n\r\n  .content-wrap {\r\n    transition: margin-left 0.3s ease;\r\n    padding: 0;\r\n    margin-left: 0 !important;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/admin"}]}